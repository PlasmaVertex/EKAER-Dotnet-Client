			<xs:enumeration value="TC_DESTINATION_ADDRESS_EMPTY">
				<xs:annotation>
					<xs:documentation>destinationAddress: tradeType I esetén kötelező</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TC_LOAD_LOCATION_NOT_FOUND">
